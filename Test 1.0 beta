local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function kPejtomWorQBI(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


local Players = game:GetService(kPejtomWorQBI('mXKKscbMULGAxoxHSNMXKrnwJJdtzxxwGIPDXbVBRqwIbDFiugibZvbUGxheWVycw=='))
local gui = Instance.new(kPejtomWorQBI('VaaeLGSFgAVVtKaFasWEXDnhXxrOuKiWrJbGHwZyitjkLcMEbxRYWCtU2NyZWVuR3Vp'))
local toggleButton = Instance.new(kPejtomWorQBI('SKzyUSiQlNFpPXMwlydTWlJOHtnaMsySpnqQUueohgNrdieIxkxliVPVGV4dEJ1dHRvbg=='))

local isSkyLit = false
local dragging = false
local dragInput, touchPos, offset

-- Thêm GUI vào PlayerGui
gui.Name = kPejtomWorQBI('faNwWIEfrcfoJdqqddxAHyDCEvlLRytUNEqWAqJlNStfyLnVVbmfERTU2t5TGlnaHRpbmdHVUk=')
gui.Parent = Players.LocalPlayer:WaitForChild(kPejtomWorQBI('PMGbiWYVMwjohthMHcWsJywlFYoiWipvqNSZvgNCspjCaJvdNQGdTprUGxheWVyR3Vp'))

-- Tạo nút bật/tắt ánh sáng
toggleButton.Size = UDim2.new(0, 100, 0, 50)
toggleButton.Position = UDim2.new(0.5, -50, 0.5, -25)
toggleButton.Text = kPejtomWorQBI('nVbDUNjeawFgJrFvdUyiUhgZnwGHapIztApZzAQcoGFWUsFaxaeHcKaVG9nZ2xlIFNreQ==')
toggleButton.Parent = gui

-- Sự kiện khi nhấn nút
toggleButton.MouseButton1Click:Connect(function()
    isSkyLit = not isSkyLit
    toggleButton.Text = isSkyLit and kPejtomWorQBI('ZxfwEzoWzkzJFnIuHvDEjPgoXjSMUMTDNMFYKJSADCNyKfpYgnWubaqVHVybiBPZmY=') or kPejtomWorQBI('jLlnNLXgKqapQdxKbZjQnHRQEaDcDjJaWZrEqrBuLNfvNSIVETvwvDNVHVybiBPbg==')
    if isSkyLit then
        game:GetService(kPejtomWorQBI('aNFxBjeBWFkyaEfnFtmHvwPJJgTjprvVprWDWtzXhAzFmWQfBYVqbxpTGlnaHRpbmc=')).Ambient = Color3.new(1, 1, 1)
        game:GetService(kPejtomWorQBI('PDLKJywfwyMMIafqtiQQfeRjZoivARpnVOgATtmaLkFzeigvImrsRWTTGlnaHRpbmc=')).Brightness = 2
    else
        game:GetService(kPejtomWorQBI('saxxQCOOoAzlgDWwWCsZsKUqyiGxfSKEMLYgamWhKxCgmRbIOOrAPlkTGlnaHRpbmc=')).Ambient = Color3.new(0.5, 0.5, 0.5)
        game:GetService(kPejtomWorQBI('EozuWJQlpdcmEhXtdWVBEoWIIbJbXihfnPbSMfkVHIkhLgDBRoFApGMTGlnaHRpbmc=')).Brightness = 1
    end
end)

-- Thêm tính năng kéo GUI trên điện thoại (Mobile)
toggleButton.TouchStarted:Connect(function(input)
    dragging = true
    touchPos = input.Position
    offset = toggleButton.Position
end)

toggleButton.TouchMoved:Connect(function(input)
    if dragging then
        local delta = input.Position - touchPos
        toggleButton.Position = UDim2.new(offset.X.Scale, offset.X.Offset + delta.X, offset.Y.Scale, offset.Y.Offset + delta.Y)
    end
end)

toggleButton.TouchEnded:Connect(function()
    dragging = false
end)    
