local RunService = game:GetService(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('dQQXzBbAYuvxiHwygnjrAlJRNaSMNNHShyqyhLvHSJJxZnyhSBwbPUiUnVuU2VydmljZQ=='))

-- GUI Setup
local player = Players.LocalPlayer
local screenGui = Instance.new(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('uZYpBKAHWELWMibFegbTBnZIuySsHpGJYdOzsULwplQvttwmcRsYrGiU2NyZWVuR3Vp'), player:WaitForChild(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('jiiiyLaWxeuGdSBEJUvGsvdNwFrqBbTEtaqpPMDUfVaBvHKVHvtPNbUUGxheWVyR3Vp')))
local timeStopButton = Instance.new(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('vAdQtzAGHukERhEhBbcmLDqAQaNXNydzmKnLXzhhLPQkdZcEmbKAdEZVGV4dEJ1dHRvbg=='), screenGui)

-- Button Properties
timeStopButton.Size = UDim2.new(0, 200, 0, 50)
timeStopButton.Position = UDim2.new(0.5, -100, 0.5, -25)
timeStopButton.Text = WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('nwfkiRidJlqsByLJPEmrtqWLAVwbbSyBWyBjqGQzRDeadnbZplXkLJiVGltZSBTdG9w')
timeStopButton.BackgroundColor3 = Color3.new(1, 0, 0)

-- Variables
local timeStopped = false
local originalVelocity = {}

-- Function to freeze time
local function freezeTime()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer then
            local character = player.Character
            if character and character:FindFirstChild(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('XPebyoRJLiVvmYNvArSbMzeUJxrwpSmbBmidWfREMbsleKhgZwCsxXSSHVtYW5vaWQ=')) then
                originalVelocity[player] = character.HumanoidRootPart.Velocity
                character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                character.Humanoid.WalkSpeed = 0
                character.Humanoid.JumpPower = 0
            end
        end
    end
end

-- Function to unfreeze time
local function unfreezeTime()
    for player, velocity in pairs(originalVelocity) do
        if player.Character and player.Character:FindFirstChild(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('MfCISrElcvKDrkwkQpkvzdWOtlNaMmkMHsIYmIIZFZyPWIqTrTwOiCzSHVtYW5vaWQ=')) then
            player.Character.HumanoidRootPart.Velocity = velocity
            player.Character.Humanoid.WalkSpeed = 16
            player.Character.Humanoid.JumpPower = 50
        end
    end
    originalVelocity = {}
end

-- Button Click Event
timeStopButton.MouseButton1Click:Connect(function()
    if not timeStopped then
        timeStopped = true
        freezeTime()
        timeStopButton.Text = WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('FyUaSFfZwciiJyxAKTITIxArQrLvGWXTWWXBBvHpjrZgCTeMkQarPbpVGltZSBSZXN1bWU=')
    else
        timeStopped = false
        unfreezeTime()
        timeStopButton.Text = WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('eDGJxBQNuJkiPqyJjRntyJYdWnYswCtPwOCCzCAEBqQtktcvsrnOgpSVGltZSBTdG9w')
    end
end)

-- Error Handling
local function safeExecute(func)
    local success, err = pcall(func)
    if not success then
        warn(WocpBdwMWOTNeEnBxukpRiCFcuoVMhLwMnuDWXuuUHQhIuErKUjJrFZn('FkWvWxRcGfKqeXxMvUyuYNwLHQznucGKRgGUwCinyTBrAJLnyQzZFXGQW4gZXJyb3Igb2NjdXJyZWQ6IA==') .. err)
    end
end

-- Main Loop
RunService.RenderStepped:Connect(function()
    safeExecute(function()
        if timeStopped then
            -- Additional logic can be added here if needed
        end
    end)
end)
    
