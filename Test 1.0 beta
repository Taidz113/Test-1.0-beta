local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
function XvEMDQXRNYTYBCvmnujejOAqEpqMZLCgKEtqoboEsnLl(data) m=string.sub(data, 0, 55) data=data:gsub(m,'')

data = string.gsub(data, '[^'..b..'=]', '') return (data:gsub('.', function(x) if (x == '=') then return '' end local r,f='',(b:find(x)-1) for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end return r; end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x) if (#x ~= 8) then return '' end local c=0 for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end return string.char(c) end)) end


 


local function setSkyToNoon()
    -- Attempt to change the sky properties
    local success, err = pcall(function()
        -- Access the Lighting service
        local lighting = game:GetService(XvEMDQXRNYTYBCvmnujejOAqEpqMZLCgKEtqoboEsnLl('NoGpoPcqXhVrFMEtNVgHPZHWznFtYMlAHoiHPuXOmwfDTrwEHOSvkbwTGlnaHRpbmc='))
        
        -- Set the sky properties to create a noon effect
        lighting.Ambient = Color3.fromRGB(255, 255, 255) -- Bright white ambient light
        lighting.Brightness = 2 -- Increase brightness
        lighting.TimeOfDay = XvEMDQXRNYTYBCvmnujejOAqEpqMZLCgKEtqoboEsnLl('oLlUlygRgLreaRpnJvTLkrZGpXSjWAnJJbtSHTbYBEGbVoLmgSaavAZMTQ6MDA6MDA=') -- Set time to 2 PM for noon-like effect
    end)

    -- Error handling
    if not success then
        warn(XvEMDQXRNYTYBCvmnujejOAqEpqMZLCgKEtqoboEsnLl('vNOBmBvgYDisuqFFuxcrIsTdzyVCOfjypWGXTDLehpADHyQMLOXTquNRmFpbGVkIHRvIHNldCBza3kgdG8gbm9vbjog') .. err)
    end
end

-- Infinite loop to continuously light up the sky
while true do
    setSkyToNoon()
    wait(0.001) -- Wait for 0.001 seconds before the next iteration
end
   
